{
  "name": "@kuus/gatsby-plugin-i18n",
  "version": "0.7.2",
  "description": "",
  "author": "kuus <kunderikuus@gmail.com> (http://kunderikuus.net)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "i18n",
    "internationalisation",
    "localisation",
    "locale",
    "translation"
  ],
  "scripts": {
    "prepublishOnly": "npm run format",
    "format": "npx prettier --write **/*.{js,jsx}",
    "_postpublish": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag $PACKAGE_VERSION && git push --tags"
  },
  "main": "index.js",
  "dependencies": {
    "@formatjs/icu-messageformat-parser": "^2.0.6",
    "@formatjs/intl-pluralrules": "^4.0.27",
    "@formatjs/intl-relativetimeformat": "^9.1.6",
    "comment-parser": "^1.1.5",
    "intl": "^1.2.5",
    "js-yaml": "^4.1.0",
    "react-intl": "^5.20.2"
  },
  "peerDependencies": {
    "@types/react-helmet": "^6.1.1",
    "gatsby": "^3.x.x",
    "react": "^17.0.x",
    "react-helmet": "^6.1.0",
    "webpack": "^5.39.0"
  },
  "devDependencies": {
    "prettier": "^2.3.1"
  }
}
